<p id="notice"><%= notice %></p>
<h1>QucikBase</h1>

<% column_array = @column.split('.') %>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Task</th>
      <th>Tax Item</th>
      <th>Agency</th>
      <th>Specific Tax item Category</th>
      <th>Status</th>
      <th>Online change</th>
      


    </tr>
  </thead>

  <tbody>
   <% @data.each do |data|  %>
    <%  sec = (data[column_array[0]["1"]].to_f / 1000).to_i  %>
      <tr>
        <!-- <td><%= @column.split('.')[0] %></td> -->
        
        <td><%=  Time.at(sec.to_i).strftime("%m-%d-%Y") %></td>
        
        <td><%= data[column_array[1]["3"]] %></td> 
        <td><%= data[column_array[2]["7"]] %></td>
        <td><%= data[column_array[3]["6"]] %></td>
        <td><%= data[column_array[4]["8"]] %></td>
        <td><%= data[column_array[5]["14"]] %></td>
        <td><%= data[column_array[6]["9"]] %></td>


      </tr>
    <% end %>
  </tbody>
</table>